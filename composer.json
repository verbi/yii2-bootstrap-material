{
    "name": "verbi/yii2-bootstrap-material",
    "description": "Yii2 package for bootstrap material design.",
    "keywords": [
        "yii2", "bootstrap", "material"
    ],
    "type": "yii2-extension",
    "license": "GPLv3",
    "support": {
        "issues": "https://github.com/verbi/yii2-bootstrap-material/issues",
        "source": "https://github.com/verbi/yii2-bootstrap-material"
    },
    "authors": [
        {
            "name": "Philip Verbist",
            "email": "philip.verbist@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "bower-asset/materialize": "*"
    },
    "autoload": {
        "psr-4": {
            "verbi\\yii2BootstrapMaterial\\": "src/"
        }
    }
}